<tr>
  <td><input class="reconcile_checkbox" id="<%= "#{entry_type}_#{reconcile_entry.id}" %>" name="entries[<%= reconcile_entry.id %>]" <%== 'checked="checked"' if @entries && @entries.include?(reconcile_entry.id) %> type="checkbox" value="1" /></td>
<td><%= reconcile_entry.date %></td>
<td><%= reconcile_entry.reference %></td>
<td class="name"><%= reconcile_entry.entity.name if reconcile_entry.entity %></td>
<td class="money" id="amount_<%= reconcile_entry.id %>"><%= reconcile_entry.money_amount %></td>
</tr>
