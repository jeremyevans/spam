<% @pagetitle = "Income &amp; Expense" %>

<table class="sparse">
<thead><tr><th>Month</th><th>Income</th><th>Expense</th><th>Profit/Loss</th></tr></thead>
<tbody>
<% @months.each do |month| -%>
  <tr><td><%= month.month %></td><td class="money"><%= month.income %></td><td class="money"><%= month.expense %></td><td class="money"><%= month.profit %></td></tr>
<% end%>
</tbody></table>